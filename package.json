{
  "name": "tradenext",
  "version": "1.0.0",
  "description": "This bundle contains starter code to add to your existing `TradeNext` repository: - Postgres/Timescale DB migrations - Node TypeScript ingestion script to consume NSE-style CSV and upsert into Timescale - Next.js starter pages: dashboard, holdings, company profile (demo charts) - Piotroski F-score SQL function + TypeScript helper - docker-compose for local dev (Postgres+Timescale, Redis, API, Frontend) - AI TODO checklist for implementing remaining features",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "prisma": "prisma",
    "scripts": "scripts"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "node scripts/dev-local-cross.js",
    "dev:clean": "bash scripts/dev-clean.sh",
    "dev:only-host": "bash scripts/dev-only-host.sh",
    "local": "npm run dev:only-host && npm run dev",
    "local:clean": "npm run dev:clean && npm run dev:only-host && npm run dev",
    "build": "npx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "ingest:run": "NODE_ENV=development node ./scripts/runIngest.js",
    "postinstall": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name add_tradenext_models",
    "prisma:studio": "prisma studio",
    "timescale:enable": "ts-node scripts/enable_timescale.ts",
    "db:up": "bash scripts/dev-db-up.sh",
    "db:down": "bash scripts/dev-db-down.sh",
    "db:enable-timescale": "npx ts-node scripts/enable_timescale.ts",
    "docker:terminate:win": "powershell -ExecutionPolicy Bypass -File scripts/docker-terminate.ps1",
    "docker:terminate": "bash scripts/docker-terminate.sh"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.0.1",
    "@prisma/extension-accelerate": "^1.3.0",
    "chart.js": "^4.5.1",
    "csv-parse": "^6.1.0",
    "next": "15.4.8",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@netlify/plugin-nextjs": "^5.14.7",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^7.1.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luckyhegde6/TradeNext.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/luckyhegde6/TradeNext/issues"
  },
  "homepage": "https://github.com/luckyhegde6/TradeNext#readme"
}